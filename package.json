{
  "name": "gpt-vis-ssr-server",
  "version": "1.0.0",
  "description": "Image generation service using GPT-Vis SSR",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-server.js",
    "docker:build": "docker build -t gpt-vis-ssr-server .",
    "docker:local": "docker-compose -f docker-compose.local.yml up -d",
    "docker:full": "docker-compose up -d",
    "docker:stop": "docker-compose down && docker-compose -f docker-compose.local.yml down",
    "docker:logs": "docker-compose logs -f gpt-vis-server",
    "docker:test": "./docker-test.sh"
  },
  "dependencies": {
    "@antv/gpt-vis-ssr": "^0.1.9",
    "@aws-sdk/client-s3": "^3.450.0",
    "canvas": "^2.11.2",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "keywords": ["express", "gpt-vis", "image-generation", "chart"],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "config": {
    "canvas_binary_host_mirror": "https://registry.npmmirror.com/-/binary/canvas/"
  },
  "author": "",
  "license": "MIT"
}
